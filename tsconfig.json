{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "dfx-generated/counter": [
        ".dfx/local/canisters/counter/counter.js"
      ],
      "dfx-generated/profile": [
        ".dfx/local/canisters/profile/profile.js"
      ],
      "dfx-generated/contract": [
        ".dfx/local/canisters/contract/contract.js"
      ]
    }
  },
  "include": [
    "./src"
  ]
}
